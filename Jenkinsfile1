    agent any
    stages {
            
        stage('1') {
            steps {
                echo 'Hello 1'
            }
        }
        
        stage('2') {
            steps {
                echo 'Hello 2'
            }
        }
        
        stage('3') {
            steps {
                echo 'Hello 3'
            }
        }
    }
}
